			} else if (noteDiff > Conductor.safeZoneOffset * 0.4){
				daRating = 'good';
				if (FCrate > 3) FCrate = 3;
				score = 200;
			} else if (noteDiff > Conductor.safeZoneOffset * 0.2){
				daRating = 'good';
				if (FCrate > 3) FCrate = 3;
				score = 300;
			}
			
			if (scrores.length > 100){ // can't be too memory intensive
				scrores = [Std.int(Accuracy * 3.5)];
