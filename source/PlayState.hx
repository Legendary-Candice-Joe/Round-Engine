			}
			
			scrores.push(score);
			
			var fakeAccuracy:Int = 0;
			for (i in 0...scrores.length)
				fakeAccuracy += scrores[i];
			if (!cache) Accuracy = (fakeAccuracy / scrores.length) / 3.5;
			if (cache) scrores = [];
			if (cache) FCrate = 0;
		}
		
		prevDiff = noteDiff;
			
		if(!cache) songScore += score;

		/* if (combo > 60)
				daRating = 'sick';
			else if (combo > 12)
				daRating = 'good'
			else if (combo > 4)
				daRating = 'bad';
		 */

