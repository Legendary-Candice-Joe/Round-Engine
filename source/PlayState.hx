				if (FCrate > 3) FCrate = 3;
				score = 200;
			}
			
			if (FCrate <= 0) FCrate = 4;
			
			if (Accuracy < 0) Accuracy = 100;
			
			if (noteDiff < prevDiff && score >= 200)
				Accuracy += 1.7;
			else if(Math.abs(noteDiff - prevDiff) >= 2)
				Accuracy -= 1.45;
				
				
			Accuracy = Math.max(1, Math.min(100, Accuracy));
		} else {
			if (FCrate == 0) FCrate = 4;
			
			if (noteDiff > Conductor.safeZoneOffset * 1){
				daRating = 'shit';
				if (FCrate > 1) FCrate = 1;
				score = 40;
			} else if (noteDiff > Conductor.safeZoneOffset * 0.9){
				daRating = 'shit';
				if (FCrate > 1) FCrate = 1;
				score = 75;
			} else if (noteDiff > Conductor.safeZoneOffset * 0.8){
				daRating = 'bad';
				if (FCrate > 2) FCrate = 2;
				score = 100;
			} else if (noteDiff > Conductor.safeZoneOffset * 0.75){
				daRating = 'bad';
				if (FCrate > 2) FCrate = 2;
